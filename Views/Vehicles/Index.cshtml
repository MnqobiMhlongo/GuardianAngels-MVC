@model IEnumerable<GuardianAngels.Models.Vehicle>

@{
    ViewBag.Title = "Index";
    Layout = "~/Views/Shared/newLayout1.cshtml";
}
<br />
<h2>DbSchoolVehiclees</h2>


<br/>
<table class="table">
    <tr>
        <th>

        </th>
        <th>
            @Html.DisplayNameFor(model => model.School.SchoolName)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Name)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Plate)
        </th>
       @* <th>
            @Html.DisplayNameFor(model => model.image)
        </th>*@
        <th>Actions</th>
    </tr>

@foreach (var item in Model) {
    <tr>
        
        <td>
            <img src="~/images/busicon.png" width="25" height="25" />
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.School.SchoolName)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Name)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Plate)
        </td>
        @*    <td>
            @Html.DisplayFor(modelItem => item.image)
        </td>*@
        <td>
            @Html.ActionLink("Edit", "Edit", new { id = item.VehicleId }) |
            @Html.ActionLink("Details", "Details", new { id = item.VehicleId }) |
            @Html.ActionLink("Delete", "Delete", new { id = item.VehicleId })
        </td>
       
    </tr>
}

</table>
